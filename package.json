{
  "name": "@patrik_hajek/admin",
  "version": "1.0.0",
  "description": "Project template for TOMAKE",
  "author": "TOMAKE",
  "license": "MIT",
  "scripts": {
    "start": "nuxt start",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "husky install",
    "postinstall": "nuxt prepare && yarn types",
    "format": "prettier --write .",
    "lint": "eslint --ext .js,.vue,.ts . --ignore-path .gitignore",
    "lintfix": "eslint --ext .js,.vue,.ts . --ignore-path .gitignore --fix",
    "tsc-check": "tsc --noEmit",
    "prisma:run": "ts-node-esm ./src/server/scripts/run.mts",
    "prisma-types": "ts-node-esm ./src/server/scripts/types.mts",
    "trpc-types": "npx tsc --declaration --strict  --pretty ./src/server/trpc/routers/index.ts --outFile ./src/types/server.js --emitDeclarationOnly --lib esnext,dom",
    "import-types": "ts-node-esm ./src/server/scripts/import.mts",
    "types": "yarn import-types && yarn trpc-types && yarn prisma-types && yarn import-types"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/google-fonts": "^3.0.0-0",
    "@nuxtjs/tailwindcss": "^6.7.0",
    "@types/nodemailer": "^6.4.8",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vue-macros/nuxt": "^1.2.6",
    "@vueuse/core": "^9.13.0",
    "@vueuse/nuxt": "^9.13.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-tailwindcss": "^3.8.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nuxt": "^3.5.0",
    "nuxt-headlessui": "^1.1.4",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.11",
    "@prisma/client": "^4.14.0",
    "@t3-oss/env-nuxt": "^0.3.1",
    "@trpc/client": "^10.25.0",
    "@trpc/server": "^10.25.0",
    "@vueup/vue-quill": "^1.2.0",
    "blurhash": "^2.0.5",
    "buffer": "^6.0.3",
    "gsap": "^3.11.4",
    "nodemailer": "^6.9.2",
    "pinia": "^2.1.3",
    "prisma": "^4.14.0",
    "sass": "^1.58.1",
    "trpc-nuxt": "^0.9.0",
    "typescript": "^5.0.4",
    "zod": "^3.21.4"
  }
}
